{
  "manifest_version": 3,
  "name": ".NET Health Check Parser",
  "description": "Chrome extension to parse dotnet health checks",
  "version": "0.0.1",
  "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
  },
  "permissions": ["activeTab"],
  "content_scripts": [{
      "js": ["parser.js"],
      "matches": ["<all_urls>"]
  }],
  "web_accessible_resources": [
    {
        "resources": ["parser.css"],
        "matches": ["<all_urls>"]
    }
  ]
}